<?xml version='1.0' encoding='utf-8'?>
<widget id="com.soap.sabee" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="AndroidLaunchMode" value="singleTask" />
    <name>PSUSecurity</name>
    <description>
       Application for PSU PHUKET Security
    </description>
    <author email="s5930213055@phuket.psu.ac.th">
        TEAM PSU PHUKET SE59
        Mrs.Nattida  Noo-Boon  5930213010,
        Mrs.Nisarat  Saelim    5930213021,
        Mrs.Waritsara Bambuttook 5930213022,
        Mrs.Radtanaporn  Choeikan  5930213024,
        Mr.Arunpong Somlok    5930213026,
        Mr.Patiphan Wibunchat 5930213055
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <preference name="android-minSdkVersion" value="19" />
        <allow-intent href="market:*" />
        <icon density="mdpi" src="res/icon/android/android-icon-36x36.png" />
        <icon density="ldpi" src="res/icon/android/android-icon-48x48.png" />
        <icon density="hdpi" src="res/icon/android/android-icon-72x72.png" />
        <icon density="xhdpi" src="res/icon/android/android-icon-96x96.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-qrscanner" spec="^2.6.0" />
    <plugin name="cordova-plugin-buildinfo" spec="2.0.2" />
    <plugin name="cordova-universal-links-plugin" spec="~1.2.1" />
    <plugin name="cordova-plugin-browsertab" spec="0.2.0" />
    <plugin name="cordova-plugin-inappbrowser" spec="3.0.0" />
    <plugin name="cordova-plugin-customurlscheme" spec="4.3.0">
        <variable name="URL_SCHEME" value="com.firebase.cordova" />
    </plugin>
    <plugin name="cordova-plugin-geolocation" spec="^4.0.1" />
    <engine name="browser" spec="^5.0.4" />
    <engine name="android" spec="^7.0.0" />
    <engine name="ios" spec="^4.5.5" />
</widget>
